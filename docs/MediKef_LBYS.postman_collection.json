    {
      "name": "LisBox Integration",
      "item": [
        {
          "name": "Receive Test Results",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "X-API-Key",
                "value": "LISBOX_SECRET_KEY_2024",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"DeviceId\": \"COBAS_C311_01\",\n  \"SampleBarcode\": \"BAR2024001\",\n  \"TestResults\": [\n    {\n      \"TestCode\": \"GLU\",\n      \"TestName\": \"Glukoz\",\n      \"ResultValue\": \"95\",\n      \"ResultNumeric\": 95.0,\n      \"Unit\": \"mg/dL\",\n      \"ReferenceRange\": \"70-110\",\n      \"Flag\": \"N\",\n      \"ResultDateTime\": \"2024-12-28T10:30:00Z\"\n    },\n    {\n      \"TestCode\": \"CREA\",\n      \"TestName\": \"Kreatinin\",\n      \"ResultValue\": \"0.9\",\n      \"ResultNumeric\": 0.9,\n      \"Unit\": \"mg/dL\",\n      \"ReferenceRange\": \"0.7-1.3\",\n      \"Flag\": \"N\",\n      \"ResultDateTime\": \"2024-12-28T10:30:00Z\"\n    }\n  ],\n  \"Status\": \"Final\",\n  \"Timestamp\": \"2024-12-28T10:30:00Z\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/LisBox/receive-results",
              "host": ["{{baseUrl}}"],
              "path": ["LisBox", "receive-results"]
            },
            "description": "LisBox middleware'den cihaz test sonuçlarını alır"
          }
        }
      ]
    },
